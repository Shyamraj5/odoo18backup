<odoo>
    <data>
        <record id="action_individual_driver_report_xlsx" model="ir.actions.report">
            <field name="name">Driver Report</field>
            <field name="model">area.driver</field>
            <field name="report_type">xlsx</field>
            <field name="report_name">individual_driver_report_xlsx</field>
            <field name="report_file">individual_driver_report_xlsx</field>
            <field name="print_report_name">Driver Report Excel</field>
            <field name="binding_model_id" eval="False"/>
            <field name="binding_type">report</field>
        </record>

       
        <record id="action_individual_driver_report_pdf" model="ir.actions.report">
            <field name="name">Driver Report</field>
            <field name="model">area.driver</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">diet.individual_driver_report_template</field>
            <field name="report_file">diet.individual_driver_report_template</field>
            <field name="binding_model_id" ref="model_area_driver"/>
            <field name="binding_type">report</field>
            <field name="paperformat_id" ref="diet.daily_meals_report_paperformat"/>
        </record>
    </data>
</odoo>