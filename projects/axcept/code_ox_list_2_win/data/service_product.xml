<odoo>
    <data noupdate="1">
        <record id="default_product_category" model="product.category">
            <field name="name">Service Product</field>
        </record>
        
        <record id="registration_service_product_corn" model="product.template">
            <field name="name">Registration</field>
            <field name="default_code">REGISTRATION</field>
            <field name="type">service</field>
            <field name="categ_id" ref="default_product_category"/>
            <field name="sale_ok" eval="True"/>
            <field name="purchase_ok" eval="False"/>
        </record>

        <record id="renewal_service_product_corn" model="product.template">
            <field name="name">Renewal</field>
            <field name="default_code">RENEWAL</field>
            <field name="type">service</field>
            <field name="categ_id" ref="default_product_category"/>
            <field name="sale_ok" eval="True"/>
            <field name="purchase_ok" eval="False"/>
        </record>

        <record id="advertisement_service_product_corn" model="product.template">
            <field name="name">Advertisement</field>
            <field name="default_code">ADVERTISEMENT</field>
            <field name="type">service</field>
            <field name="categ_id" ref="default_product_category"/>
            <field name="sale_ok" eval="True"/>
            <field name="purchase_ok" eval="False"/>
        </record>

        <record id="referral_commission_service_product_corn" model="product.template">
            <field name="name">Referral Commission</field>
            <field name="default_code">REFERRAL_COMMISSION</field>
            <field name="type">service</field>
            <field name="categ_id" ref="default_product_category"/>
            <field name="sale_ok" eval="False"/>
            <field name="purchase_ok" eval="True"/>
        </record>

        <record id="listing_service_product_corn" model="product.template">
            <field name="name">Listing</field>
            <field name="default_code">LISTING</field>
            <field name="type">service</field>
            <field name="categ_id" ref="default_product_category"/>
            <field name="sale_ok" eval="True"/>
            <field name="purchase_ok" eval="False"/>
        </record>
    </data>
</odoo>