<odoo>
    <record id="product_template_form_inherit_sale_return" model="ir.ui.view">
        <field name="name">product.template.form.inherit.sale.return</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_only_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='property_account_income_id']" position="after">
                <field name="credit_note_account_id"/>
            </xpath>
        </field>
    </record>
</odoo>
