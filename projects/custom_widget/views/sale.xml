<odoo>

    <record id="sale_order_view_form_inherit_sale_return" model="ir.ui.view">
        <field name="name">sale.order.view.form.inherit.sale.return</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='partner_id']" position="after">
                <field name="my_number" widget="integer_stepper"/>
                <field name="is_sale_order"/>
                <field name="text" widget="my_text_field"/>
            </xpath>
        </field>
    </record>

</odoo>
