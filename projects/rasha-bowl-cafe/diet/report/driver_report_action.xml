<odoo>
    <data>
        <record id="action_driver_report" model="ir.actions.report">
            <field name="name">Driver Delivery Daily Report</field>
            <field name="model">driver.report.wizard</field>
            <field name="report_type">xlsx</field>
            <field name="report_name">driver_report_xlsx</field>
            <field name="report_file">driver_report_xlsx</field>
            <field name="print_report_name">Driver Report Excel</field>
            <field name="binding_model_id" eval="False"/>
            <field name="binding_type">report</field>
        </record>

        <record id="action_driver_report_preview" model="ir.actions.report">
            <field name="name">Driver Delivery Daily Report</field>
            <field name="model">driver.report.wizard</field>
            <field name="report_type">qweb-html</field>
            <field name="report_name">diet.driver_report_template</field>
            <field name="report_file">diet.driver_report_template</field>
            <field name="binding_model_id" ref="model_driver_report_wizard"/>
            <field name="binding_type">report</field>
            <field name="paperformat_id" ref="diet.individual_customer_daily_meals_report_paperformat"/>
        </record>

        <record id="action_driver_report_pdf" model="ir.actions.report">
            <field name="name">Driver Delivery Daily Report</field>
            <field name="model">driver.report.wizard</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">diet.driver_report_template</field>
            <field name="report_file">diet.driver_report_template</field>
            <field name="binding_model_id" ref="model_driver_report_wizard"/>
            <field name="binding_type">report</field>
            <field name="paperformat_id" ref="diet.individual_customer_daily_meals_report_paperformat"/>
        </record>
    </data>
</odoo>