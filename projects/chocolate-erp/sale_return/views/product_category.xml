<odoo>
    <record id="product_category_form_inherit_sale_return" model="ir.ui.view">
        <field name="name">product.category.form.inherit.sale.return</field>
        <field name="model">product.category</field>
        <field name="inherit_id" ref="account.view_category_property_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='property_account_income_categ_id']" position="after">
                <field name="credit_note_account_id"/>
            </xpath>
        </field>
    </record>
</odoo>
