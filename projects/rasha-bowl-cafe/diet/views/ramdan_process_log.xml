<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="ramdan_process_log_view_form" model="ir.ui.view">
        <field name="name">ramdan.process.log.view.form</field>
        <field name="model">ramdan.process.log</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="processed_datetime"/>
                        <field name="processed_by"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="ramdan_process_log_view_tree" model="ir.ui.view">
        <field name="name">ramdan.process.log.view.list</field>
        <field name="model">ramdan.process.log</field>
        <field name="arch" type="xml">
            <list create="0" edit="0">
                <field name="processed_datetime"/>
                <field name="processed_by"/>
            </list>
        </field>
    </record>

    <record id="ramdan_process_log_action" model="ir.actions.act_window">
        <field name="name">Ramdan Process Log</field>
        <field name="res_model">ramdan.process.log</field>
        <field name="view_mode">list,form</field>
    </record>


</odoo>