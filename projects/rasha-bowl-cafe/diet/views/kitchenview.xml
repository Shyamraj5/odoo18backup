<odoo>

    <record id="kitchen_type_form" model="ir.ui.view">
        <field name="name">kitchen Type Form</field>
        <field name="model">kitchen.type</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="kitchen_type_tree" model="ir.ui.view">
        <field name="name">kitchen Type list</field>
        <field name="model">kitchen.type</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
            </list>
        </field>
    </record>

    <record id="kitchen_type_search" model="ir.ui.view">
        <field name="name">kitchen Type Search</field>
        <field name="model">kitchen.type</field>
        <field name="arch" type="xml">
            <search>
                <field name="name"/>
            </search>
        </field>
    </record>

    <record id="kitchen_type_action" model="ir.actions.act_window">
        <field name="name">Kitchen Types</field>
        <field name="res_model">kitchen.type</field>
        <field name="view_mode">list,form,search</field>
    </record>



    


</odoo>