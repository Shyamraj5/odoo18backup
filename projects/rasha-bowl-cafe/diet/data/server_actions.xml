<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="change_driver_action" model="ir.actions.server">
        <field name="name">Change Driver</field>
        <field name="model_id" ref="diet.model_driver_order"/>
        <field name="binding_model_id" ref="diet.model_driver_order"/>
        <field name="state">code</field>
        <field name="code">model.open_driver_change_wizard()</field>
    </record>

    <record id="action_freeze_all" model="ir.actions.server">
        <field name="name">Freeze Selected</field>
        <field name="model_id" ref="diet.model_diet_subscription_order"/>
        <field name="binding_model_id" ref="diet.model_diet_subscription_order"/>
        <field name="state">code</field>
        <field name="code">model.action_freeze_all()</field>
    </record>


</odoo>
