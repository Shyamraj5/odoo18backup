<odoo>
    <record id="branch_transfer_out_company_rule" model="ir.rule">
        <field name="name">Branch Transfer Out multi-company</field>
        <field name="model_id" ref="model_branch_transfer_out"></field>
        <field name="domain_force">[('company_id', 'in', company_ids)]</field>
    </record>

    <record id="branch_transfer_in_company_rule" model="ir.rule">
        <field name="name">Branch Transfer In multi-company</field>
        <field name="model_id" ref="model_branch_transfer_in"></field>
        <field name="domain_force">[('company_id', 'in', company_ids)]</field>
    </record>

    <record id="module_category_branch_sale" model="ir.module.category">
        <field name="name">Branch Sale</field>
        <field name="description">Branch Sale.</field>
        <field name="sequence">9</field>
    </record>

    <record id="group_branch_sale" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="module_category_branch_sale"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        <field name="comment">The user will be able to do Branch Sale.</field>
    </record>
</odoo>