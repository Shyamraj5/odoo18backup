<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_commission_tracking_list" model="ir.ui.view">
        <field name="name">incentive.bonus.list</field>
        <field name="model">incentive.bonus</field>
        <field name="arch" type="xml">
            <list string="Incentives">
                <field name="commission_plan_id"/>
                <field name="sales_person"/>
                <field name="total_sales_amount"/>
                <field name="target_amount"/>
                <field name="date"/>
                <field name="period_name"/>
                <field name="incentive_bonus"/>
            </list>
        </field>
    </record>

    <record id="action_incentive_bonus" model="ir.actions.act_window">
        <field name="name">Incentives</field>
        <field name="res_model">incentive.bonus</field>
        <field name="view_mode">list</field>
    </record>

    
</odoo>