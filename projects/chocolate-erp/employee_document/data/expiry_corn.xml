<odoo>
    <record id="ir_cron_document_tracking_alerts" model="ir.cron">
        <field name="name">Document Tracking Alerts</field>
        <field name="model_id" ref="model_document_tracking"/>
        <field name="state">code</field>
        <field name="code">model.check_expiry_soon_status()</field>
        <field name="interval_type">days</field>
        <field name="interval_number">1</field>
        <field name="active">True</field>
    </record>
</odoo>