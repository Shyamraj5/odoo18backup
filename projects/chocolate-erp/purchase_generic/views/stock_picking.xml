<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
        <!-- stock picking form - change label -->
        <record id="view_picking_form_custom" model="ir.ui.view">
            <field name="name">stock.picking.form.custom</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='move_ids_without_package']//list//field[@name='product_uom_qty']" position="attributes">
                    <attribute name="string">Ordered Quantity</attribute>
                </xpath>
            </field>
        </record>
    </data>

</odoo>
